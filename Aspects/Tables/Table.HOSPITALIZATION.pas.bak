procedure THOSPITALIZATIONItem.InsertHOSPITALIZATION;
var
  CollType: TCollectionsType;
  metaPosition, dataPosition, PropPosition: cardinal;
  FPosMetaData, FLenMetaData, FPosData, FLenData: Cardinal;
  propIndx: TPropertyIndex;
  pCardinalData: ^Cardinal;
  pWordData: ^Word;
begin
  CollType := Aspects.Types.TCollectionsType.ctHOSPITALIZATION;
  pCardinalData := pointer(PByte(buf));
  FPosMetaData := pCardinalData^;
  pCardinalData := pointer(PByte(buf) + 4);
  FLenMetaData := pCardinalData^;
  metaPosition :=  FPosMetaData + FLenMetaData;

  pCardinalData := pointer(PByte(buf) + 8);
  FPosData := pCardinalData^;
  pCardinalData := pointer(PByte(buf) + 12);
  FLenData := pCardinalData^;
  dataPosition :=  FPosData + FLenData;
  SaveAnyStreamCommand(@PRecord.setProp, SizeOf(PRecord.setProp), CollType, toInsert, FVersion, metaPosition + 4);
  case FVersion of
    0:
    begin
      pWordData := pointer(PByte(buf) + metaPosition);
      pWordData^  := word(CollType);
      pWordData := pointer(PByte(buf) + metaPosition + 2);
      pWordData^  := FVersion;
      inc(metaPosition, 4);
	  Self.DataPos := metaPosition;
	  
      for propIndx := Low(TPropertyIndex) to High(TPropertyIndex) do
      begin
        if Assigned(PRecord) and (propIndx in PRecord.setProp) then
        begin
          case propIndx of
            HOSPITALIZATION_AMB_PROCEDURE: SaveData(PRecord.AMB_PROCEDURE, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_CLINICAL_PATH: SaveData(PRecord.CLINICAL_PATH, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_DIRECT_DATE: SaveData(PRecord.DIRECT_DATE, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_ID: SaveData(PRecord.ID, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_NOTES: SaveData(PRecord.NOTES, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_NRN: SaveData(PRecord.NRN, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_NUMBER: SaveData(PRecord.NUMBER, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_Logical: SaveData(TLogicalData24(PRecord.Logical), PropPosition, metaPosition, dataPosition);
          end;
        end
        else
        begin
          SaveNull(metaPosition);
        end;
      end;
      pCardinalData := pointer(PByte(buf) + 4);
      pCardinalData^  := metaPosition - FPosMetaData;
      pCardinalData := pointer(PByte(buf) + 12);
      pCardinalData^  := dataPosition - FPosData;
    end;
  end;
end;

function  THOSPITALIZATIONItem.IsFullFinded(buf: Pointer; FPosDataADB: Cardinal; coll: TCollection): Boolean;
var
  i: Integer;
  pidx:  THOSPITALIZATIONItem.TPropertyIndex;
  cot: TConditionSet;
  ATempItem: THOSPITALIZATIONItem;
begin
  Result := True;
  for i := 0 to Length(THOSPITALIZATIONColl(coll).ArrPropSearchClc) - 1 do
  begin
    if Result = false then
      Exit;
    pidx := THOSPITALIZATIONColl(coll).ArrPropSearchClc[i];
	ATempItem := THOSPITALIZATIONColl(coll).ListForFinder.Items[0];
    cot := ATempItem.ArrCondition[word(pidx)];
    begin
      case pidx of
        HOSPITALIZATION_AMB_PROCEDURE: Result := IsFinded(ATempItem.PRecord.AMB_PROCEDURE, buf, FPosDataADB, word(HOSPITALIZATION_AMB_PROCEDURE), cot);
            HOSPITALIZATION_CLINICAL_PATH: Result := IsFinded(ATempItem.PRecord.CLINICAL_PATH, buf, FPosDataADB, word(HOSPITALIZATION_CLINICAL_PATH), cot);
            HOSPITALIZATION_DIRECT_DATE: Result := IsFinded(ATempItem.PRecord.DIRECT_DATE, buf, FPosDataADB, word(HOSPITALIZATION_DIRECT_DATE), cot);
            HOSPITALIZATION_ID: Result := IsFinded(ATempItem.PRecord.ID, buf, FPosDataADB, word(HOSPITALIZATION_ID), cot);
            HOSPITALIZATION_NOTES: Result := IsFinded(ATempItem.PRecord.NOTES, buf, FPosDataADB, word(HOSPITALIZATION_NOTES), cot);
            HOSPITALIZATION_NRN: Result := IsFinded(ATempItem.PRecord.NRN, buf, FPosDataADB, word(HOSPITALIZATION_NRN), cot);
            HOSPITALIZATION_NUMBER: Result := IsFinded(ATempItem.PRecord.NUMBER, buf, FPosDataADB, word(HOSPITALIZATION_NUMBER), cot);
            HOSPITALIZATION_Logical: Result := IsFinded(TLogicalData24(ATempItem.PRecord.Logical), buf, FPosDataADB, word(HOSPITALIZATION_Logical), cot);
      end;
    end;
  end;
end;

procedure THOSPITALIZATIONItem.SaveHOSPITALIZATION(Abuf: Pointer; var dataPosition: Cardinal);
var
  pCardinalData: PCardinal;
  APosData, ALenData: Cardinal;
begin
  pCardinalData := pointer(PByte(ABuf) + 8);
  APosData := pCardinalData^;
  pCardinalData := pointer(PByte(ABuf) + 12);
  ALenData := pCardinalData^;
  dataPosition :=  ALenData + APosData;
  SaveHOSPITALIZATION(dataPosition);
end;

procedure THOSPITALIZATIONItem.SaveHOSPITALIZATION(var dataPosition: Cardinal);
var
  CollType: TCollectionsType;
  metaPosition, PropPosition: cardinal;
  propIndx: TPropertyIndex;
begin
  CollType := ctHOSPITALIZATION;
  SaveAnyStreamCommand(@PRecord.setProp, SizeOf(PRecord.setProp), CollType, toUpdate, FVersion, dataPosition);
  case FVersion of
    0:
    begin
      for propIndx := Low(TPropertyIndex) to High(TPropertyIndex) do
      begin
        if propIndx in PRecord.setProp then
        begin
          SaveHeaderData(PropPosition, dataPosition);
          metaPosition := FDataPos + 4 * Integer(propIndx);
          case propIndx of
            HOSPITALIZATION_AMB_PROCEDURE: SaveData(PRecord.AMB_PROCEDURE, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_CLINICAL_PATH: SaveData(PRecord.CLINICAL_PATH, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_DIRECT_DATE: SaveData(PRecord.DIRECT_DATE, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_ID: SaveData(PRecord.ID, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_NOTES: SaveData(PRecord.NOTES, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_NRN: SaveData(PRecord.NRN, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_NUMBER: SaveData(PRecord.NUMBER, PropPosition, metaPosition, dataPosition);
            HOSPITALIZATION_Logical: SaveData(TLogicalData24(PRecord.Logical), PropPosition, metaPosition, dataPosition);
          end;
        end
        else
        begin
          //SaveNull(metaPosition);
        end;
      end;
      Dispose(PRecord);
      PRecord := nil;
    end;
  end;
end;


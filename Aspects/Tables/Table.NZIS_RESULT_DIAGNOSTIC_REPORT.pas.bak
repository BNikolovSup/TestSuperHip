var
  RowSelect: Integer;
  prop: TNZIS_RESULT_DIAGNOSTIC_REPORTItem.TPropertyIndex;
begin
  inherited;
 
  if ARow < 0 then
  begin
    AValue := 'hhhh';
    Exit;

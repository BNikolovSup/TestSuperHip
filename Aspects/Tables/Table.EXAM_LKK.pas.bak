    EXAM_LKK_Spec5Pos: isOld :=  EXAM_LKK.getIntMap(Self.Buf, Self.posData, ACol) = StrToInt(AFieldText);
    end;
  end;
  if isOld then
  begin
    Exclude(EXAM_LKK.PRecord.setProp, TEXAM_LKKItem.TPropertyIndex(ACol));
    if EXAM_LKK.PRecord.setProp = [] then
    begin
      Dispose(EXAM_LKK.PRecord);
      EXAM_LKK.PRecord := nil;
      CntUpdates := CntUpdates - 1;
      Exit;
    end;
  end;
  Include(EXAM_LKK.PRecord.setProp, TEXAM_LKKItem.TPropertyIndex(ACol));
  case TEXAM_LKKItem.TPropertyIndex(ACol) of
    EXAM_LKK_DATA: EXAM_LKK.PRecord.DATA := StrToDate(AFieldText);
    EXAM_LKK_ID: EXAM_LKK.PRecord.ID := StrToInt(AFieldText);
    EXAM_LKK_NRN: EXAM_LKK.PRecord.NRN := AFieldText;
    EXAM_LKK_NUMBER: EXAM_LKK.PRecord.NUMBER := StrToInt(AFieldText);
    EXAM_LKK_Spec1Pos: EXAM_LKK.PRecord.Spec1Pos := StrToInt(AFieldText);
    EXAM_LKK_Spec2Pos: EXAM_LKK.PRecord.Spec2Pos := StrToInt(AFieldText);
    EXAM_LKK_Spec3Pos: EXAM_LKK.PRecord.Spec3Pos := StrToInt(AFieldText);
    EXAM_LKK_Spec4Pos: EXAM_LKK.PRecord.Spec4Pos := StrToInt(AFieldText);
    EXAM_LKK_Spec5Pos: EXAM_LKK.PRecord.Spec5Pos := StrToInt(AFieldText);
    EXAM_LKK_Logical: EXAM_LKK.PRecord.Logical := tlogicalEXAM_LKKSet(EXAM_LKK.StrToLogical08(AFieldText));
  end;
end;


    CL132_Max_interval_from_common_index: ListForFinder[0].PRecord.Max_interval_from_common_index := AText;
    CL132_Age: ListForFinder[0].PRecord.Age := AText;
    CL132_Max_Age: ListForFinder[0].PRecord.Max_Age := AText;
    CL132_Recurring: ListForFinder[0].PRecord.Recurring := AText;
    CL132_Repeat_Every_x_Years: ListForFinder[0].PRecord.Repeat_Every_x_Years := AText;

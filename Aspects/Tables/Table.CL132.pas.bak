    CL132_CL136_Mapping: ListForFinder[0].PRecord.CL136_Mapping := AText;
    CL132_CL011_Mapping: ListForFinder[0].PRecord.CL011_Mapping := AText;
    CL132_Event_Trigger: ListForFinder[0].PRecord.Event_Trigger := AText;
